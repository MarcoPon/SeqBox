local int BLOCKSIZE = 512;

BigEndian();
DisplayFormatHex();

struct BLOCK {
    struct HEADER {
        struct MAGIC {
            char    signature[3];
        } magic;
        byte    version; 
        short   CRC16;
        struct UID {
            byte    uid[6];
        } uid;
        int     blocknum;
    } header;
    byte    data[BLOCKSIZE - sizeof(header)];
} block[FileSize() / BLOCKSIZE];


